{
  "name": "y",
  "version": "1.0.0",
  "description": "Typescript course",
  "main": "index.js",
  "scripts": {
    "start": "node dist/app.js",
    "anki:chap5": "markdown-to-anki sections/section05/anki.md > anki.txt",
    "chap1:hello-world": "tsc --outFile dist/hello-world.js sections/section01/hello-world.ts && node dist/hello-world.js",
    "chap2:types01": "tsc --outFile dist/app.js sections/section02/types01.ts && node dist/app.js",
    "chap2:enums": "tsc --outFile dist/app.js sections/section02/enums.ts && node dist/app.js",
    "chap2:union": "tsc --outFile dist/app.js sections/section02/union.ts && node dist/app.js",
    "chap2:function-types": "tsc --outFile dist/app.js sections/section02/function-types.ts && node dist/app.js",
    "chap2:callbacks": "tsc --outFile dist/app.js sections/section02/callbacks.ts && node dist/app.js",
    "chap3:watch": "tsc --outFile dist/app.js --watch sections/section03/watch.ts",
    "chap3:init": "cd sections/section03/init && tsc --outDir dist/ --watch",
    "chap3:fileoptions": "cd sections/section03/fileoptions && tsc --outDir dist/ --watch",
    "chap3:target": "cd sections/section03/target && tsc --outDir dist/",
    "chap3:source": "cd sections/section03/source && tsc --outDir dist/",
    "chap3:dir": "cd sections/section03/dir && tsc",
    "chap5:basic": "cd sections/section05/basic && tsc && node dist/basic.js",
    "chap5:methods": "cd sections/section05/methods && tsc && node dist/main.js",
    "chap5:private": "cd sections/section05/private && tsc && node dist/main.js",
    "chap5:shorthand": "cd sections/section05/shorthand && tsc && node dist/main.js",
    "chap5:readonly": "cd sections/section05/readonly && tsc",
    "chap5:inheritance": "cd sections/section05/inheritance && tsc && node dist/main.js",
    "chap5:protected": "cd sections/section05/protected && tsc && node dist/main.js",
    "chap5:getset": "cd sections/section05/getset && tsc && node dist/main.js",
    "chap5:static": "cd sections/section05/static && tsc && node dist/main.js",
    "chap5:abstract": "cd sections/section05/abstract && tsc && node dist/main.js",
    "chap5:singleton": "cd sections/section05/singleton && tsc && node dist/main.js",
    "chap5:interface": "cd sections/section05/interface && tsc && node dist/main.js",
    "chap5:intvstype": "cd sections/section05/intvstype && tsc && node dist/main.js",
    "chap5:whyinterface": "cd sections/section05/whyinterface && tsc && node dist/main.js",
    "chap5:readonlyinter": "cd sections/section05/readonlyinter && tsc && node dist/main.js",
    "chap5:combineinter": "cd sections/section05/combineinter && tsc && node dist/main.js",
    "chap5:optionalprop": "cd sections/section05/optionalprop && tsc && node dist/main.js",
    "chap6:intersection": "cd sections/section06/intersection && tsc && node dist/main.js",
    "chap6:guards": "cd sections/section06/guards && tsc && node dist/main.js",
    "chap6:disc": "cd sections/section06/disc && tsc && node dist/main.js",
    "chap6:casting": "cd sections/section06/casting && tsc && node dist/main.js",
    "chap6:index": "cd sections/section06/index && tsc && node dist/main.js",
    "chap6:overloads": "cd sections/section06/overloads && tsc && node dist/main.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Andrew4d3/typescript-maxi-course.git"
